  <script>
    let selectedMovie = '';
    const seatsContainer = document.getElementById('seats-container');
    const bookingSection = document.getElementById('booking-section');
    const selectedMovieSpan = document.getElementById('selected-movie');
    const output = document.getElementById('output');

    function selectMovie(movie) {
      const city = document.getElementById('city').value;
      const theatre = document.getElementById('theatre').value;
      const time = document.getElementById('time').value;

      if (!city || !theatre || !time) {
        alert('Please select City, Theatre, and Show Time first!');
        return;
      }

      selectedMovie = movie;
      selectedMovieSpan.textContent = movie;
      bookingSection.style.display = 'block';
      output.innerHTML = '';
      generateSeats();
    }

    function generateSeats() {
      seatsContainer.innerHTML = '';
      for (let i = 1; i <= 300; i++) {
        const seat = document.createElement('div');
        seat.className = 'seat';
        seat.textContent = i;
        seat.onclick = function() {
          seat.classList.toggle('selected');
        }
        seatsContainer.appendChild(seat);
      }
    }

    function bookTickets() {
      const selectedSeats = [...document.querySelectorAll('.seat.selected')].map(s => s.textContent);
      if (selectedSeats.length === 0) {
        alert('Please select at least one seat!');
        return;
      }

      const city = document.getElementById('city').value;
      const theatre = document.getElementById('theatre').value;
      const time = document.getElementById('time').value;

      let snacks = [];
      let total = selectedSeats.length * 350; // ‚Çπ350 per seat

      if (document.getElementById('popcorn').checked) { snacks.push('Popcorn'); total += 150; }
      if (document.getElementById('cola').checked) { snacks.push('Cola'); total += 100; }
      if (document.getElementById('nachos').checked) { snacks.push('Nachos'); total += 180; }
      if (document.getElementById('pizza').checked) { snacks.push('Veg Pizza'); total += 220; }
      if (document.getElementById('samosa').checked) { snacks.push('Veg Samosa 2N'); total += 80; }

      output.innerHTML = `
        <h3>‚úÖ Booking Confirmed!</h3>
        <p><b>üé• Movie:</b> ${selectedMovie}</p>
        <p><b>üìç City:</b> ${city}</p>
        <p><b>üèõÔ∏è Theatre:</b> ${theatre}</p>
        <p><b>üïí Show Time:</b> ${time}</p>
        <p><b>üí∫ Seats:</b> ${selectedSeats.join(', ')}</p>
        <p><b>üçî Snacks:</b> ${snacks.length ? snacks.join(', ') : 'None'}</p>
        <p><b>üí∞ Total Amount:</b> ‚Çπ${total}</p>
      `;
    }
  </script>
